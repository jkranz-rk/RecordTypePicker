<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <classAccesses>
        <apexClass>RecordTypePickerController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <description>Permission Set used in Apex Tests for RecordType Picker. Assigns one of two active Record Types on Contact to validate user-context is respected</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>RecordType Picker Permissions Testing</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Contact</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <recordTypeVisibilities>
        <recordType>Contact.External</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
</PermissionSet>
